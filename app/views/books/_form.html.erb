<%= simple_form_for(book) do |f| %>
  <%= f.input :isbn %>
  <input class="btn btn-danger" type="button" id="info_search_button" value="検索">
  <%= f.input :title %>
  <%= f.input :author %>
  <%= f.input :publisherName %>
  <%= f.input :salesDate %>
  <%= f.association :categories, as: :select, multiple:true %>
  <div class="actions">
    <%= f.button :submit, :class => 'btn-primary' %>
  </div>
<% end %>
